<!DOCTYPE web-app PUBLIC
		"-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
		"http://java.sun.com/dtd/web-app_2_3.dtd" >


<web-app>

	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>index</servlet-name>
		<servlet-class>top.kxyu.web.index</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>login</servlet-name>
		<servlet-class>top.kxyu.web.login</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>reg</servlet-name>
		<servlet-class>top.kxyu.web.reg</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>mainList</servlet-name>
		<servlet-class>top.kxyu.web.mainList</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>index</servlet-name>
		<url-pattern>/index</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>login</servlet-name>
		<url-pattern>/login</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>reg</servlet-name>
		<url-pattern>/reg</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>mainList</servlet-name>
		<url-pattern>/mainList</url-pattern>
	</servlet-mapping>
</web-app>

